generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Project {
  id           String   @id @default(uuid())
  title        String
  context      String
  objectives   String
  technologies String
  results      String
  createdAt    DateTime @default(now())
  imageUrl     String
  videoUrl     String
}

model Contact {
  id      String @id @default(uuid())
  name    String
  email   String
  message String
}
